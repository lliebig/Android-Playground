<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="de.leoliebig.playground">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".patterns.mvp.userprofile.UserProfileActivityMvp" />
        <activity android:name=".patterns.mvvm.userprofile.UserProfileActivityMvvm" />
        <activity
            android:name=".RealmActivity"
            android:label="@string/title_activity_realm"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>